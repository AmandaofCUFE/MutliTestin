<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.TSmallcommentEntity" table="smallcomment" schema="testin">
        <id name="sId">
            <column name="s_id" sql-type="int(11)"/>
        </id>
        <property name="sContent">
            <column name="s_content" sql-type="varchar(255)"/>
        </property>
        <property name="sDate">
            <column name="s_date" sql-type="varchar(255)"/>
        </property>
        <property name="sLikes">
            <column name="s_likes" sql-type="int(10) unsigned zerofill"/>
        </property>
        <property name="userId">
            <column name="user_id" sql-type="int(11)"/>
        </property>
        <property name="commentId">
            <column name="comment_id" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>